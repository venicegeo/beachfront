{
  "name": "bf-ui",
  "version": "1.0.0",
  "description": "User interface for the Beachfront project",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf report dist beachfront.zip",
    "clear": "rm -rf beachfront.zip dist/ node_modules/ report/ typings/",
    "create-ssl-certs": "openssl req -newkey rsa:2048 -nodes -keyout .development_ssl_certificate.key -sha256 -x509 -days 3652 -subj /C=US/ST=Unknown/L=Unknown/O=Beachfront/OU=Development/CN=localhost -out .development_ssl_certificate.pem",
    "lint": "tslint '{src,test}/**/*.{ts,tsx}' -t verbose",
    "postinstall": "npm run typings:install",
    "prereinstall": "npm run clear",
    "prestart": "npm run reinstall",
    "reinstall": "npm install",
    "start": "npm run watch",
    "test": "karma start",
    "test:ci": "NODE_ENV=ci karma start --single-run --browsers Chrome,Firefox --reporters progress,coverage,junit",
    "test:once": "karma start --single-run",
    "typings:install": "typings install",
    "watch": "webpack-dev-server --inline --colors --history-api-fallback --https --cert .development_ssl_certificate.pem --key .development_ssl_certificate.key --host 0.0.0.0"
  },
  "author": "RadiantBlue Technologies",
  "license": "Apache-2.0",
  "engines": {
    "node": "^5.7.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "copy-webpack-plugin": "3.0.1",
    "css-loader": "0.23.1",
    "detect-browser": "2.0.0",
    "enzyme": "2.4.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "2.30.1",
    "istanbul-instrumenter-loader": "1.1.0",
    "karma": "1.1.2",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-junit-reporter": "1.1.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.0.5",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.9",
    "mocha": "3.2.0",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "0.9.1",
    "react-addons-test-utils": "15.2.1",
    "rimraf": "2.5.4",
    "sinon": "2.0.0-pre",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "tslint": "3.14.0",
    "tslint-loader": "2.1.5",
    "typescript": "2.1.4",
    "typings": "2.0.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.2"
  },
  "dependencies": {
    "axios": "0.15.3",
    "core-js": "2.4.1",
    "font-awesome": "4.6.3",
    "lodash": "4.17.4",
    "moment": "2.20.1",
    "ol": "4.1.1",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-redux": "5.0.7",
    "react-user-tour": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  }
}
